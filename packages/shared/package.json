{
  "name": "@mateatletas/shared",
  "version": "0.0.1",
  "private": true,
  "description": "Shared types and utilities for Mateatletas",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && tsc && node ./scripts/fix-dist.cjs",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "echo 'No lint configured for shared package'",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@mateatletas/contracts": "1.0.0"
  }
}
