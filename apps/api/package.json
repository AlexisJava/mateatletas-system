{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=integration.spec.ts --testPathIgnorePatterns=e2e-spec.ts",
    "test:integration": "jest --testPathPattern=integration.spec.ts --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:unit": "jest --coverage --testPathIgnorePatterns=integration.spec.ts --testPathIgnorePatterns=e2e-spec.ts",
    "test:cov:integration": "jest --coverage --testPathPattern=integration.spec.ts --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "db:migrate": "npx prisma migrate deploy",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "db:seed:prod": "NODE_ENV=production npx prisma db seed",
    "db:reset": "npx prisma migrate reset --force",
    "db:studio": "npx prisma studio",
    "detect-planificaciones": "ts-node src/planificaciones-simples/scripts/auto-detect-planificaciones.ts"
  },
  "dependencies": {
    "@keyv/redis": "^5.1.3",
    "@mateatletas/contracts": "workspace:^",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "6.18.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cache-manager": "^7.2.4",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "keyv": "^5.5.3",
    "mercadopago": "^2.9.0",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "6.18.0",
    "puppeteer": "^24.29.1",
    "qrcode": "^1.5.4",
    "redis": "^5.8.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "7.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/pdfkit": "^0.17.3",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "madge": "^8.0.0",
    "pdfkit": "^0.17.2",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!uuid)"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/*.interface.ts",
      "!**/*.dto.ts",
      "!**/*.module.ts",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/coverage/**"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text",
      "text-summary",
      "html",
      "lcov",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 65,
        "lines": 70,
        "statements": 70
      }
    },
    "testEnvironment": "node",
    "maxWorkers": "50%",
    "testTimeout": 10000
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
